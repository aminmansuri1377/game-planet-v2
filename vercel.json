{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "runtime": "nodejs",
        "regions": ["iad1"],
        "includeFiles": ["public/**", ".next/**", "node_modules/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/dashboard(.*)",
      "dest": "/dashboard",
      "continue": true
    }
  ]
}
