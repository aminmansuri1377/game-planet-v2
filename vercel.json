{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/trpc/(.*)",
      "destination": "/api/trpc"
    }
  ],
  "regions": ["iad1"],
  "functions": {
    "api/**/*.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
